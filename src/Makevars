pomdp-solve/pomdp-solve:
	(cd pomdp-solve && CXX="$(CXX)" CC="$(CC)" CFLAGS="$(CFLAGS) $(CPICFLAGS)" AR="$(AR)" RANLIB="$(RANLIB)" $(MAKE))

clean:
#	rm -f libpomdp.dll
	$(MAKE) -C pomdp-solve clean
